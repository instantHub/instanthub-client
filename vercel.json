{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "https://api.instantpick.in/sitemap.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.php$",
      "status": 404,
      "dest": "/404"
    }
  ]
}
